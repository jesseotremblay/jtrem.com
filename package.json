{
  "name": "my-site-paper-template",
  "version": "4.2.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,astro}\" --fix",
    "lint:check": "eslint \"src/**/*.{js,jsx,ts,tsx,astro}\"",
    "posse": "node posse.js",
    "posse:latest": "node posse.js",
    "posse:help": "node posse.js --help",
    "webmentions:test": "node test-webmentions.js",
    "webmentions:help": "node test-webmentions.js --help",
    "posse:test": "curl -s -I https://jtrem.com/posts/what-operator-shows-us-about-tomorrows-web | grep -E '(HTTP|Content-Type|Last-Modified)'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.7-beta.1",
    "@astrojs/rss": "^4.0.15-beta.2",
    "@atproto/api": "^0.15.14",
    "@resvg/resvg-js": "^2.6.0",
    "astro": "^6.0.0-beta.5",
    "dotenv": "^16.5.0",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "lru-cache": "^11.0.2",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.11",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@astrojs/react": "^5.0.0-beta.3",
    "@astrojs/sitemap": "^3.6.1-beta.2",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/ts-plugin": "^1.10.4",
    "@divriots/jampack": "^0.23.2",
    "@eslint/config-array": "^0.19.1",
    "@eslint/object-schema": "^2.1.5",
    "@tailwindcss/typography": "^0.5.10",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "astro-eslint-parser": "^0.16.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-react": "^7.37.3",
    "glob": "^11.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-img-size": "^1.0.1",
    "rehype-plugin-image-native-lazy-loading": "^1.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
}
